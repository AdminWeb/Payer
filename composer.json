{
  "name": "adminweb/payer",
  "description":"A new package for payments for laravel",
  "version": "0.0.12",
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "illuminate/database": "~5.6",
    "illuminate/support": "~5.6",
    "nesbot/carbon": "~1.0",
    "symfony/http-kernel": "~2.7|~3.0|~4.0",
    "ramsey/uuid": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "AdminWeb\\Payer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AdminWeb\\Payer\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "vlucas/phpdotenv": "~2.0",
    "phpunit/phpunit": "~7.0",
    "illuminate/http": "~5.6",
    "illuminate/routing": "~5.6",
    "illuminate/view": "~5.6",
    "orchestra/testbench": "3.6",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpcov": "^5.0"
  },
  "authors": [
    {
      "name": "IgorDePaula",
      "email": "principe.borodin@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "AdminWeb\\Payer\\PayerServiceProvider"
      ]
    }
  }
}
